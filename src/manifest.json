{
  "manifest_version": 3,
  "name": "tell me the title",
  "version": "1.0.0",
  "description": "",
  "author": "ヴォルガ・クルヴァール",
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/playlist?list=*"],
      "js": ["index.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["webRequest"],
  "host_permissions": [
    "https://archive.org/wayback/*",
    "https://web.archive.org/web/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "kurvan1112@gmail.com-tell-me-the-title",
      "strict_min_version": "109.0"
    }
  }
}
